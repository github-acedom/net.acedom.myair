{
  "title": {
    "en": "Set aircon fan speed"
  },
  "highlight": true,
  "args": [
    {
      "name": "device",
      "type": "device",
      "filter": "driver_id=myair_control"
    },
    {
      "name": "fan_speed",
      "type": "dropdown",
      "values": [
        {"id": "low", "label": {"en": "Low"}},
        {"id": "medium", "label": {"en": "Medium"}},
        {"id": "high", "label": {"en": "High"}},
        {"id": "auto", "label": {"en": "Auto"}}
      ]
    }
  ]
}
